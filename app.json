{
  "expo": {
    "name": "Assemblie",
    "slug": "assemblie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon_Primary.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Assemblie_FullIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#10192b"
    },
    "ios": {
      "supportsTablet": true,
      "associatedDomains": [
        "applinks:assemblie.app",
        "applinks:www.assemblie.app"
      ],
      "bundleIdentifier": "com.assemblie.assemblie",
      "buildNumber": "10",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow Assemblie to access your Face ID biometric data for secure authentication."
      },
      "appleTeamId": "42Q62H5FNQ"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icon-96.png",
        "backgroundColor": "#10192b"
      },
      "package": "com.assemblie.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.assemblie.app",
              "pathPrefix": "/connect"
            },
            {
              "scheme": "https",
              "host": "assemblie.app",
              "pathPrefix": "/connect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "versionCode": 9,
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/Assemblie_FullIcon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Assemblie to access your camera to scan QR codes for check-in and checkout."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/Icon_Primary.png",
          "color": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar to add events."
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "eas": {
        "projectId": "e9a7b152-ad5c-448a-9e71-7fcb3b2be29d"
      },
      "enableDebugInProduction": true
    },
    "scheme": "assemblie",
    "owner": "assemblie",
    "androidStatusBar": {
      "backgroundColor": "#2f3131",
      "translucent": true,
      "barStyle": "light-content"
    }
  }
}