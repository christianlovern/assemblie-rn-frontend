{
  "expo": {
    "entryPoint": "./AppContext.js",
    "name": "CongreGate",
    "slug": "CongreGate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-dark.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/CongreGate.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "associatedDomains": ["applinks:congregate.app"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/CongreGate.png",
        "backgroundColor": "#ffffff"
      },
      "plugins": [
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar to add events."
        }
      ]
    ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "congregate.app",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/CongreGate.png"
    },
    "scheme": "congregate"
  }
}
